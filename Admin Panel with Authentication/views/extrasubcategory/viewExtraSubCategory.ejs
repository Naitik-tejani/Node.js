<%- include('../header') %>

    <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-12 d-flex no-block align-items-center">
                    <h4 class="page-title">Tables</h4>
                    <div class="ml-auto text-right">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Library</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="col-12">
                    <% if(locals?.message?.success) { %>
                        <div class="alert alert-success w-50" role="alert">
                            <%= locals?.message?.success %>
                        </div>
                        <% } %>
                            <div class="card">
                                <div class="card-body">
                                    <h5 class="card-title m-b-0">ExtraSubCategory Data</h5>
                                </div>
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">Id</th>
                                            <th scope="col">CategoryName</th>
                                            <th scope="col">SubcategoryName</th>
                                            <th scope="col">ExtraSubcategoryName</th>
                                            <th scope="col">Action</th>
                                            <th scope="col">Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% extraSubCategory.map((val,i)=> { %>
                                            <tr>
                                                <th scope="row">
                                                    <%= ++i %>
                                                </th>
                                                <td>
                                                    <%= val?.categoryId?.category %>
                                                </td>
                                                <td>
                                                    <%= val.subcategoryId?.subcategory %>
                                                </td>
                                                <td>
                                                    <%= val?.extraSubCategory %>
                                                </td>
                                                <td>
                                                    <a href="/extrasubcategory/deleteextrasubcategory?delId=<%= val?.id %>"
                                                        class="btn btn-danger btn-sm">Delete&nbsp;&nbsp;<i
                                                        class="fa fa-trash"></i></a>


                                                    <a href="/extrasubcategory/editextrasubcategory?editId=<%= val?.id %>"
                                                        class="btn btn-warning btn-sm">Edit&nbsp;&nbsp;<i
                                                        class="fa fa-edit"></i></a>
                                                </td>
                                                <td>

                                                    <% if(val?.status==="active" ) { %>
                                                        <a href="/extrasubcategory/changestatus?stId=<%= val?.id %>&status=deactive"
                                                            class="btn btn-success btn-sm text-capitalize">
                                                            <%= val?.status %>
                                                        </a>
                                                        <% } else { %>
                                                            <a href="/extrasubcategory/changestatus?stId=<%= val?.id %>&status=active"
                                                                class="btn btn-danger btn-sm text-capitalize">
                                                                <%= val?.status %>
                                                            </a>
                                                            <% } %>

                                                </td>
                                            </tr>
                                            <% }) %>
                                    </tbody>
                                </table>
                            </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End PAge Content -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Right sidebar -->
            <!-- ============================================================== -->
            <!-- .right-sidebar -->
            <!-- ============================================================== -->
            <!-- End Right sidebar -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        <footer class="footer text-center">
            All Rights Reserved by Matrix-admin. Designed and Developed by <a
                href="https://wrappixel.com">WrapPixel</a>.
        </footer>
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
    </div>

    <%- include('../footer') %>